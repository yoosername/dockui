{
  "name": "dockui",
  "version": "2.0.0",
  "description": "plug and play web apps using docker containers",
  "author": "https://github.com/yoosername",
  "repository": {
    "type": "git",
    "url": "git://github.com/yoosername/dockui.git"
  },
  "keywords": [
    "docker",
    "composition",
    "plugin",
    "webapp",
    "ui"
  ],
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "test": "./node_modules/mocha/bin/mocha src/**/*spec.js --reporter spec",
    "jshint": "./node_modules/jshint/bin/jshint src/**/*.js",
    "coverage": "nyc ./node_modules/mocha/bin/mocha src/**/*spec.js --reporter spec",
    "coveralls": "nyc npm test && nyc report --reporter=text-lcov | coveralls",
    "generate-docs": "node ./node_modules/documentation/bin/documentation.js readme index.js --section=API && mocha src/**/*spec.js --reporter=markdown > ./src/tests.md"
  },
  "dependencies": {
    "dockerode": "^2.5.3",
    "events": "^1.1.1",
    "util": "^0.10.3"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "coveralls": "^3.0.0",
    "documentation": "^5.3.3",
    "jshint": "^2.9.5",
    "mocha": "^4.0.1",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^11.3.0",
    "sinon": "^4.1.1",
    "sinon-chai": "^2.14.0"
  },
  "template": {
    "forceLanguage": "en"
  }
}
