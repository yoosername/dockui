{
  "name": "dockui",
  "version": "0.0.1-Alpha",
  "description": "Create Plug and Play front/back end microservices using containers",
  "author": "https://github.com/yoosername",
  "repository": {
    "type": "git",
    "url": "git://github.com/yoosername/dockui.git"
  },
  "keywords": [
    "docker",
    "composition",
    "plug-and-play",
    "app",
    "webapp",
    "ui",
    "microservices",
    "frontend",
    "backend",
    "api-gateway"
  ],
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "cli": "node ./src/cli/cli.js",
    "test": "./node_modules/mocha/bin/mocha 'src/**/*spec.js' --reporter spec",
    "test-dev": "npx nodemon -x npm test",
    "jshint": "./node_modules/jshint/bin/jshint src/**/*.js",
    "coverage": "nyc ./node_modules/mocha/bin/mocha 'src/**/*spec.js' --reporter spec",
    "coveralls": "nyc npm test && nyc report --reporter=text-lcov | coveralls",
    "generate-index": "./tools/build_index.sh > index.js",
    "generate-test-docs": "mocha 'src/**/*spec.js' --reporter=markdown > TESTS.md",
    "generate-api-docs": "node ./node_modules/documentation/bin/documentation.js build -f md -o API.md",
    "generate-docs": "npm run generate-index && npm run generate-test-docs && npm run generate-api-docs",
    "serve-docs": "npm run generate-index && node ./node_modules/documentation/bin/documentation.js serve --format html"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "dockerode": "^2.5.7",
    "events": "^1.1.1",
    "js-yaml": "^3.12.1",
    "nodegit": "^0.24.0",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@types/proxyquire": "^1.3.28",
    "chai": "^4.2.0",
    "coveralls": "^3.0.2",
    "documentation": "^5.5.0",
    "jshint": "^2.9.7",
    "mocha": "^4.1.0",
    "mocha-lcov-reporter": "^1.3.0",
    "moxios": "^0.4.0",
    "nyc": "^11.9.0",
    "proxyquire": "^2.1.0",
    "sinon": "^4.5.0",
    "sinon-chai": "^2.14.0"
  },
  "template": {
    "forceLanguage": "en"
  }
}
