{
  "name": "dockui",
  "version": "2.0.0",
  "description": "plug and play web apps using docker containers",
  "author": "https://github.com/yoosername",
  "repository": {
    "type": "git",
    "url": "git://github.com/yoosername/dockui.git"
  },
  "keywords": [
    "docker",
    "composition",
    "app",
    "webapp",
    "ui"
  ],
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "cli": "node ./src/cli/cli.js",
    "test": "./node_modules/mocha/bin/mocha 'src/**/*spec.js' --reporter spec",
    "test-dev": "npx nodemon -x npm test",
    "jshint": "./node_modules/jshint/bin/jshint src/**/*.js",
    "coverage": "nyc ./node_modules/mocha/bin/mocha 'src/**/*spec.js' --reporter spec",
    "coveralls": "nyc npm test && nyc report --reporter=text-lcov | coveralls",
    "generate-test-docs": "mocha 'src/**/*spec.js' --reporter=markdown > ./src/TESTS.md",
    "generate-index": "./tools/build_index.sh > index.js",
    "generate-api-docs": "node ./node_modules/documentation/bin/documentation.js build index.js -f html -o ./docs --theme node_modules/documentation-devseed-theme",
    "generate-docs": "npm run generate-index && npm run generate-test-docs && npm run generate-api-docs",
    "serve-docs": "npm run generate-index && node ./node_modules/documentation/bin/documentation.js serve --format html --theme node_modules/documentation-devseed-theme"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "dockerode": "^2.5.7",
    "events": "^1.1.1",
    "js-yaml": "^3.12.0",
    "superagent": "^3.8.3",
    "util": "^0.10.4",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "coveralls": "^3.0.2",
    "documentation": "^5.5.0",
    "documentation-devseed-theme": "^0.1.4",
    "jshint": "^2.9.7",
    "mocha": "^4.1.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nock": "^9.6.1",
    "nyc": "^11.9.0",
    "sinon": "^4.5.0",
    "sinon-chai": "^2.14.0"
  },
  "template": {
    "forceLanguage": "en"
  }
}
